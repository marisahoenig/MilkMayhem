   BULLET * b = &bullets[i];
   if(!b->active) {
    b->active = 1;
    b->row = (((p.row) >> 8) + (p.height/2));
    b->col = p.col + p.width;
    if (p.moveState == PLEFT) {
     b->cd = -b->cd;
    }
    break;
   }
  }
 }

 for(int i = 0; i < 5; i++) {
