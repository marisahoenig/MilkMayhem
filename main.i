void update() {
 p.rd += p.racc;
    p.row += p.rd;

 if ((~((*(volatile unsigned int *)0x04000130)) & 16) || (~((*(volatile unsigned int *)0x04000130)) & 32)) {
  p.aniCounter++;
  if ((~((*(volatile unsigned int *)0x04000130)) & 16)) {
   p.direction = RIGHT;


    hOff += p.cd;



   if (fridge.active) {
    fridge.cd = -2;
    updateFridge(&fridge);
   }
  }
  if ((~((*(volatile unsigned int *)0x04000130)) & 32)) {
   p.direction = LEFT;


    hOff -= p.cd;



   if (fridge.active) {
    fridge.cd = 2;
    updateFridge(&fridge);
   }
  }
  if (p.aniCounter % 10 == 0) {

   if (p.currFrame < 2) {
    p.currFrame += 1;
   } else if (p.currFrame == 2) {
    p.currFrame = 1;
   }
  }
 } else {

  currFrame = 0;
 }

 if (((p.row) >> 8) >= 160 - p.height - 1) {
        p.row = ((160 - p.height - 1) << 8);
        p.rd = 0;
    }
    if (((p.row) >> 8) <= 0) {
        p.row = ((0) << 8);
        p.rd = -p.rd;
    }
    if (p.col + p.width > 240) {
     p.col = 240-p.width;
    }
    if (p.col < 0) {
     p.col = 0;
    }

 if ((!(~oldButtons&(2))&&(~buttons&(2))) && p.rd > -p.stopRange && p.rd < p.stopRange) {
  p.rd = -p.maxJumpSpeed;
 }


 if (!(time++ % timeToNextCat)) {
  for(int i = 0; i < 2; i++) {
   CAT * c = &cats[i];
   if(!c->active) {
    c->active = 1;

    c->col = 220;
    timeToNextCat = rand()%300 + 87;
    break;
   }
  }
 }


 for (int i = 0; i < 2; i++) {
  CAT * c = &cats[i];
  if (c->active) {
   updateCat(c);

   if (!chocolateMilk && collisionEnemyPlayer(&p, c)) {
    playSoundB(meow, 4874, 11025, 0);
    c->active = 0;
